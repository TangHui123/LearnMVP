<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.talkweb.tanghui.learnsample"
          xmlns:android="http://schemas.android.com/apk/res/android">
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.talkweb.tanghui.learnsample.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/avatar"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.talkweb.tanghui.learnsample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.talkweb.tanghui.learnsample.TestActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.talkweb.tanghui.learnsample.animation.AniActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name="com.talkweb.tanghui.learnsample.TestOkhttpActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name="com.talkweb.tanghui.learnsample.MyViewGroupActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".PaintActivity"
                  android:screenOrientation="portrait"/><!-- ATTENTION: git rebase This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>

</manifest>